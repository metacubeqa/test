	<test name="Conversation AI Inbox And Features">
		<parameter name="suiteId" value="63"></parameter>
		<groups>
			<run>
				<include name="Regression" />
				<include name="MediumPriority" />
			</run>
		</groups>

		<classes>
			<class name="support.cases.conversationAIReact.FeatureAccessibility"></class>
			<class name="support.cases.conversationAIReact.InboxCases"></class>
			<class name="support.cases.conversationAIReact.CallPlayerCases"></class>
			<class name="support.cases.conversationAIReact.CAICoachCases"></class>
			<class name="support.cases.conversationAIReact.FeatureAccessibility"></class>
		</classes>
	</test>

	<test name="Conversation AI Settings">
		<parameter name="suiteId" value="63"></parameter>
		<groups>
			<run>
				<include name="Regression" />
				<include name="MediumPriority" />
			</run>
		</groups>

		<packages>
			<package
				name="support.cases.conversationAIReact.caiSettings.*">
			</package>
		</packages>

	</test>
