<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">


<suite name="SFDC Click To Call Classic">

	<listeners>
		<listener class-name="utility.listeners.AnnotationTransformer" />
	</listeners>
	<test name="Pre Requisites">
	 <groups>
		<run>
			<include name="Regression" />
		</run>
	 </groups>
	 <classes>
		<class name="support.cases.prerequisite.PreRequisites" />
	 </classes>
	</test>
	<test name="Click To Call Classic">
		<parameter name="suiteId" value="17"></parameter>
		<parameter name="ctcRunType" value="classic"></parameter>
		<groups>
			<run>
				<include name="Regression" />
			</run>
		</groups>
		<classes>
			<class name="softphone.cases.clickToCall.ClickToCall"></class>
		</classes>
	</test>
</suite> 
