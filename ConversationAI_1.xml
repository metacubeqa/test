<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Conversation AI Suite">
	<listeners>
		<listener
			class-name="utility.listeners.AnnotationTransformer" />
	</listeners>
	<test name="Conversation AI search">
		<parameter name="suiteId" value="63"></parameter>
		<classes>
			<class name="support.cases.conversationAIReact.CAIReactSearch"></class>
		</classes>
	</test>
	<test name="Conversation AI Boolean Filters">
		<parameter name="suiteId" value="63"></parameter>
		<classes>
			<class name="support.cases.conversationAIReact.CAIBooleanFilters"></class>
		</classes>
	</test>
	<test name="Conversation AI Library">
		<parameter name="suiteId" value="63"></parameter>
		<classes>
			<class name="support.cases.conversationAIReact.CAILibraryCases"></class>
		</classes>
	</test>
	<test name="Conversation AI Saved Search">
		<parameter name="suiteId" value="63"></parameter>
		<classes>
			<class name="support.cases.conversationAIReact.CAISavedSearch"></class>
		</classes>
	</test>
</suite>
