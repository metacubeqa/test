<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="QA CAI Insights Reports Suite">
	<listeners>
		<listener
			class-name="utility.listeners.AnnotationTransformer" />
	</listeners>

	<parameter name="metacubeOrg" value="metacubeOrg"></parameter>

	<test name="QA CAI Insights Reports Test">
		<parameter name="suiteId" value="5394"></parameter>
		<parameter name="drivername" value="adminDriver"></parameter>
		<groups>
			<run>
				<include name="Regression" />
			</run>
		</groups>
		
		<packages>
			<package name="report.cases.caiReports.*">
			</package>
		</packages>
		
		<classes>
			<class name="report.cases.caiReports.CAIReportsHealthCheck">
				<methods>
					<exclude name=".*" />
				</methods>
			</class>
		</classes>
		
	</test>
</suite>
