<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Reports Admin Role">
	<listeners>
		<listener
			class-name="utility.listeners.AnnotationTransformer" />
	</listeners>

	<test name="Reports">
		<parameter name="suiteId" value="47"></parameter>
		<parameter name="drivername" value="adminDriver"></parameter>
		<groups>
			<run>
				<include name="Regression" />
				<include name="MediumPriority" />
				<exclude name="SupportOnly" />
				<exclude name ="ExcludeProd"/>
			</run>
		</groups>
		
		<packages>
			<package name="report.cases.adminReports.*">
			</package>
		</packages>
		
	</test>
</suite>
