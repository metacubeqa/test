<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">


<suite name="ACD Call Flow">

	<listeners>
		<listener class-name="utility.listeners.AnnotationTransformer" />
	</listeners>
	<test name="Pre Requisites">
		<groups>
			<run>
				<include name="Regression" />
			</run>
		</groups>
		<classes>
			<class name="support.cases.prerequisite.PreRequisites" />
		</classes>
	</test>
	
	<test name="ACD Call Flow">
		<parameter name="suiteId" value="10"></parameter>
		<groups>
			<run>
				<include name="Regression" />
				<include name="MediumPriority" />
			</run>
		</groups>

		<classes>
			<class name="softphone.base.SoftphoneBase"></class>
			<class name="softphone.cases.callFlow.ACDCallFlow"></class>
		</classes>
	</test>
</suite> <!-- Suite -->
