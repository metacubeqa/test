<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="RegressionSuite">
	<test name="Conference Calling">
		<parameter name="suiteId" value="15"></parameter>
		<groups>
			<run>
				<include name="Regression" />
			</run>
		</groups>
		<classes>
			<class name="support.cases.prerequisite.PreRequisites" />
			<class name="softphone.cases.conference.ConferenceCalling" />
		</classes>
	</test>
	<test name="Conference Calling With Call Forwarding">
	<parameter name="suiteId" value="15"></parameter>
		<groups>
			<run>
				<include name="Regression" />
			</run>
		</groups>
		<classes>
			<class name="softphone.cases.conference.ConferenceCallingCallForwarding" />
		</classes>
	</test>
		<test name="Call Forwarding With Stay Connected">
		<parameter name="suiteId" value="15"></parameter>
		<groups>
			<run>
				<include name="Regression" />
			</run>
		</groups>
		<classes>
			<class name="softphone.cases.conference.ConferenceCallWithStayConnected" />
		</classes>
	</test>
	
	<test name="Team Section">
	<parameter name="suiteId" value="16"></parameter>
		<groups>
			<run>
				<include name="Regression" />
			</run>
		</groups>
		<classes>
			<class name="softphone.cases.team.TeamSection" />
		</classes>
	</test>
	
	<test name="Team Section with CallForwarding">
	<parameter name="suiteId" value="16"></parameter>
		<groups>
			<run>
				<include name="Regression" />
			</run>
		</groups>
		<classes>
			<class name="softphone.cases.team.TeamSectionCallForwarding" />
		</classes>
	</test>

	<test name="Team Section with Stay Connected">
	<parameter name="suiteId" value="16"></parameter>
		<groups>
			<run>
				<include name="Regression" />
			</run>
		</groups>
		<classes>
			<class name="softphone.cases.team.TeamWithStayConnected" />
		</classes>
	</test>
	
	
</suite> <!-- RegressionSuite -->
