<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">


<suite name="SoftPhone Recording Forwarding Transfer">

	<listeners>
		<listener class-name="utility.listeners.AnnotationTransformer" />
	</listeners>
	<test name="Pre Requisites">
		<groups>
			<run>
				<include name="Regression" />
			</run>
		</groups>
		<classes>
			<class name="support.cases.prerequisite.PreRequisites" />
		</classes>
	</test>
	<test name="SoftPhone CallRecordings">
		<parameter name="suiteId" value="10"></parameter>
		<classes>
			<class name="softphone.base.SoftphoneBase"></class>
			<class name="softphone.cases.softPhoneFunctional.callRecording.CallRecordingOverview"></class>
			<class name="softphone.cases.softPhoneFunctional.callRecording.CallRecordingsVisibility"></class>
			<class name="softphone.cases.softPhoneFunctional.callRecording.RestrictRecordingDownload"></class>
			<class name="softphone.cases.softPhoneFunctional.callRecording.DisplayRecordingStatus"></class>
			<class name="softphone.cases.softPhoneFunctional.callRecording.CallRecordingOnTeam"></class>
			<class name="softphone.cases.softPhoneFunctional.callRecording.CallRecordingUnitedStates"></class>
		</classes>
	</test> 
	<test name="SoftPhone CallTransfer">
		<parameter name="suiteId" value="10"></parameter>

		<classes>
			<class name="softphone.cases.softPhoneFunctional.CallTransfer"></class>
		</classes>
	</test>
	<test name="SoftPhone CallFowarding">
		<parameter name="suiteId" value="10"></parameter>
		<classes>
			<class name="softphone.cases.softPhoneFunctional.CallFowarding"></class>
		</classes>
	</test>
		<test name="SoftPhone LoadingDialer">
		<parameter name="suiteId" value="10"></parameter>
		<classes>
			<class name="softphone.cases.softPhoneFunctional.LoadingDialer"></class>
		</classes>
	</test>
</suite> <!-- Suite -->