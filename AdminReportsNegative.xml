<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Reports Admin Negative Role">
	<listeners>
		<listener
			class-name="utility.listeners.AnnotationTransformer" />
	</listeners>

	<parameter name="metacubeOrg" value="metacubeOrg"></parameter>

	<test name="Admin Reports Negative">
		<parameter name="suiteId" value="47"></parameter>
		<parameter name="drivername" value="adminDriver"></parameter>
		<groups>
			<run>
				<include name="Regression" />
				<include name="MediumPriority" />
				<exclude name="SupportOnly" />
			</run>
		</groups>
		
		<packages>
			<package name="report.cases.adminReports.adminReportsNegative.*">
			</package>
		</packages>
		
	</test>
</suite>
