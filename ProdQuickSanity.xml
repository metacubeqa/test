<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">


<suite name="Quick Sanity">

	<listeners>
		<listener class-name="utility.listeners.AnnotationTransformer" />
	</listeners>
	<test name="Quick Sanity">
		<parameter name="suiteId" value="33"></parameter>
		<groups>
			<run>
				<include name="QuickSanity" />
				<exclude name="ExludeForProd"></exclude>
			</run>
		</groups>
		<classes>
			<class name="softphone.base.SoftphoneBase"></class>
			<class name="softphone.cases.conference.ConferenceCalling"></class>
			<class name="softphone.cases.conference.ConferenceCallingCallForwarding"></class>
			<class name="softphone.cases.QuickSanity"></class>
			<class name="softphone.cases.softPhoneFunctional.callTools.CallToolsNewTask"></class>
			<class name="softphone.cases.softPhoneFunctional.QueueCalls"></class>
			<class name="softphone.cases.softPhoneFunctional.AddContactsLeads"></class>
			<class name="softphone.cases.team.TeamSection"></class>
		</classes>
	</test>
</suite> <!-- Suite -->